<div class="form-group">
    <label>Name</label>
    <input class="au-input au-input--full" value="{{data.userData.userName}}" type="text" name="name" placeholder="Name"
        required>
</div>
<div class="form-group">
    <label>Address</label>
    <input class="au-input au-input--full" type="text" name="address" placeholder="Address"
        value="{{data.userData.userAddress}}" required>
</div>
<div class="form-group">
    <label>Birth Year</label>
    <input class="au-input au-input--full" type="number" name="year" placeholder="Birth Year"
        value="{{data.userData.userYear}}" required>
</div>
<div class="form-group">
    <label>Contact</label>
    <input class="au-input au-input--full" type="number" name="contact" placeholder="Contact Number"
        value="{{data.userData.userContact}}" required>
</div>
<button type="submit" class="btn btn-lg btn-info btn-block">UPDATE</button>